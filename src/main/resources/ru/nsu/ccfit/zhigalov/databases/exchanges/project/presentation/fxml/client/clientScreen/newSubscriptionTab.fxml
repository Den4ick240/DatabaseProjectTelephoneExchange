<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="node" alignment="CENTER" style="-fx-background-color: #ffaaff;" xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.nsu.ccfit.zhigalov.databases.exchanges.project.presentation.client.signedin.tabs.NewSubscriptionTabView">
    <fx:include fx:id="requestAttributes" source="requestAttributes.fxml"/>
    <Button fx:id="createRequestButton" mnemonicParsing="false" prefHeight="49.0" prefWidth="176.0"
            text="Подать заявку">
        <VBox.margin>
            <Insets top="30.0"/>
        </VBox.margin>
    </Button>
    <Label alignment="CENTER" prefHeight="70.0" prefWidth="304.0"
           text="Ваша заявка будет помещена в очередь. Когда подойдет ваша очередь вам будет выдан телефонный номер, и вы сможете увидеть данные о вашей подписке."
           textAlignment="CENTER" wrapText="true">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
    </Label>
    <Label fx:id="responseLabel" alignment="CENTER" prefHeight="70.0" prefWidth="199.0" text="Responce"
           textAlignment="CENTER" wrapText="true">
        <VBox.margin>
            <Insets top="20.0"/>
        </VBox.margin>
    </Label>
</VBox>
